{
	"name": "package-general",
	"version": "0.1.0",
	"description": "General Template to get started with TypeScript CLI Tool..",
	"license": "MIT",
	"author": "VGSEVEN <contact@vgseven.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/silver-radium/templates.git",
		"directory": "silver-radium/templates"
	},
	"keywords": ["create-radium", "react", "nextjs", "tailwindcss", "radix-ui", "astro", "solidjs", "million"],
	"module": "index.ts",
	"type": "module",
	"exports": "./dist/index.js",
	"bin": {
		"cli-general": "./dist/index.js"
	},
	"files": ["dist", "README.md", "package.json"],
	"engines": {
		"bun": ">=1.1"
	},
	"scripts": {
		"lint": "biome lint . --write",
		"finetune": "biome format . --write",
		"build": "bun build index.ts --outdir ./dist --target node --format esm",
		"build:prepare": "bun run finetune && bun run lint && bun run build",
		"release": "changeset publish --tag latest",
		"prepare": "husky"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"picocolors": "^1.0.1",
		"fs-extra": "^11.2.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.5",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.14.8",
		"husky": "^9.0.11"
	},
	"peerDependencies": {
		"typescript": "^5.5.2"
	}
}
