{
  "name": "radium-init",
  "private": false,
  "version": "0.1.0",
  "description": "Initialize any new project in seconds with your tools..",
  "license": "MIT",
  "author": "vgseven <contact@vgseven.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/silver-radium/init.git",
    "directory": "silver-radium/init"
  },
  "homepage": "https://github.com/silver-radium/init",
  "keywords": [
    "radium",
    "react",
    "nextjs",
    "tailwindcss",
    "radix-ui",
    "astro",
    "solidjs",
    "million"
  ],
  "module": "index.ts",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "radium-init": "./dist/index.js",
    "init": "./dist/index.js",
    "radium": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "jiti ./src/index.ts",
